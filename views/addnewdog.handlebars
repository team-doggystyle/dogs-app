  <h2>
    Add a dog to our database:
  </h2>
  <h2>
    Add a Dog
  </h2>
  <form name="addnewdog" action="/dogs/addnewdog" class="create-form addnewdog" onsubmit="return validateForm()" method="post">
    <div class="form-helper">
      {{#if name}}
        <h1>{{name}} has been added!</h1>
      {{/if}}
    </div>
    <div class="form-group">
      <label for="ca">
        Dog Name:
      </label>
      <input name="name" type="text">
        <label for="ca">
          Dog Breed:
        </label>
        <input name="breed" type="text">
        </input>
      </input>
    </div>
    <div class="form-group">
      <span>
        Age:
      </span>
      <div class="custom-select" style="width:200px;">
        <select name="age_years">
          <option value="0">
            Years:
          </option>
          <option value="1">
            0
          </option>
          <option value="2">
            1
          </option>
          <option value="3">
            2
          </option>
          <option value="4">
            3
          </option>
          <option value="5">
            4
          </option>
          <option value="6">
            5
          </option>
          <option value="7">
            6
          </option>
          <option value="8">
            7
          </option>
          <option value="9">
            8
          </option>
          <option value="10">
            9
          </option>
          <option value="11">
            10
          </option>
          <option value="12">
            11
          </option>
          <option value="13">
            12
          </option>
          <option value="14">
            13
          </option>
          <option value="15">
            14
          </option>
          <option value="16">
            15
          </option>
          <option value="17">
            16
          </option>
          <option value="18">
            17
          </option>
          <option value="19">
            18
          </option>
          <option value="20">
            19
          </option>
          <option value="21">
            20+
          </option>
        </select>
      </div>
      <div class="custom-select" style="width:200px;">
        <select name="age_months">
          <option value="0">
            Months:
          </option>
          <option value="1">
            0
          </option>
          <option value="2">
            1
          </option>
          <option value="3">
            2
          </option>
          <option value="4">
            3
          </option>
          <option value="5">
            4
          </option>
          <option value="6">
            5
          </option>
          <option value="7">
            6
          </option>
          <option value="8">
            7
          </option>
          <option value="9">
            8
          </option>
          <option value="10">
            9
          </option>
          <option value="11">
            10
          </option>
          <option value="12">
            11
          </option>
          <option value="13">
            12
          </option>
        </select>
      </div>
    </div>
    <button type="submit">
      Add Dog
    </button>
  </form>
  <h2>
    Recently adopted dogs:
  </h2>
  <ul>
    {{#each dogs}}
    {{#if adopted}}
      {{> dogs/dog-block adopted=false}}
    {{/if}}
  {{/each}}
  </ul>
  <!-- Custom Script -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js">
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.0/moment.min.js" type="text/javascript">
  </script>
  <script src="/assets/js/addnewdog.js">
  </script>
</body>
